{
  "name": "update-changelog",
  "version": "0.0.0",
  "description": "Update changelog github action",
  "main": "dist/index.js",
  "type": "module",
  "author": "Andrzej Wasiak",
  "license": "MIT",
  "scripts": {
    "build": "rm -fr dist && npm exec --package=@vercel/ncc -- ncc build -o dist -m -t --no-source-map-register -C --license licenses.txt src/index.ts",
    "test": "vitest --run",
    "test-local": "npm run build && npm run test && rm -fr dist"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "github-action-ts-run-api": "^3.0.4",
    "typescript": "^5.3.2",
    "unplugin-swc": "^1.4.3",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@js-joda/core": "^5.6.1"
  }
}
